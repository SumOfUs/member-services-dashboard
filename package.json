{
  "name": "member-services-dashboard",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "parser": "babylon",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "react-scripts build",
    "ci:coveralls": "cat coverage/lcov.info | coveralls",
    "ci:test": "jest --coverage",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.21.0",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-gravatar": "^2.6.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-select": "^1.0.0-rc.10",
    "reactbulma": "^1.1.1",
    "redux": "^3.7.2",
    "source-map-explorer": "^1.5.0",
    "validator": "^9.1.2"
  }
}
